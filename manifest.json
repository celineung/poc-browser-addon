{
  "manifest_version": 2,
  "name": "POC Addon Popup",
  "description": "Extension navigateur qui affiche une popup",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "gatsby-icon.png",
    "default_title": "My Addon",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "storage"
  ]
}
